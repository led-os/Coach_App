<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.store.home.mine.ApplySettledInActivity">

    <include layout="@layout/title_bar_layout"/>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="企业名称"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_cpy_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textCursorDrawable="@null"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入真实企业名称"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>


            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="品牌名称"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_brand_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textCursorDrawable="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入真实品牌名称"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>
            <LinearLayout
                android:id="@+id/ll_location"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="所在地区"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
                <TextView
                    android:id="@+id/tv_location"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:textSize="@dimen/padding_13"
                    android:hint="请选择"
                    android:gravity="end|center_vertical"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/padding_10"
                    android:layout_marginEnd="@dimen/padding_14"
                    android:src="@drawable/icon_shop_right"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>


            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="详细地址"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_detail_location"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textCursorDrawable="@null"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入详细地址"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>

            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="业务负责人"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_charge"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:textCursorDrawable="@null"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入业务负责人姓名"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>

            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="手机号"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_phone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textCursorDrawable="@null"
                    android:inputType="number"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入手机号"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>


            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="常用微信号"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_weixin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入真实微信号"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>


            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="商标号"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>


                <EditText
                    android:id="@+id/et_trademark"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:layout_marginEnd="@dimen/padding_24"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/padding_13"
                    android:hint="请输入商标注册书上的商标号"
                    android:gravity="end|center_vertical"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>

            <LinearLayout
                android:id="@+id/ll_type"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_50">

                <TextView
                    android:text="请选择套餐类型"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textSize="@dimen/text_size_15"
                    android:textColor="@color/color_333333"/>

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"/>
                <TextView
                    android:id="@+id/tv_type"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:textColor="@color/color_333333"
                    android:textColorHint="@color/color_dddddd"
                    android:textSize="@dimen/padding_13"
                    android:hint="请选择"
                    android:gravity="end|center_vertical"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/padding_10"
                    android:layout_marginEnd="@dimen/padding_14"
                    android:src="@drawable/icon_shop_right"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_0.5"
                android:layout_marginStart="@dimen/padding_14"
                android:layout_marginEnd="@dimen/padding_24"
                android:background="@color/color_dddddd"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="认证材料"
                android:textSize="@dimen/text_size_15"
                android:textColor="@color/color_333333"
                android:layout_marginTop="@dimen/padding_27"
                android:layout_marginStart="@dimen/padding_14"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/rel_yyzz"
                    android:layout_below="@+id/tv_condition_des"
                    android:layout_marginStart="@dimen/padding_14"
                    android:layout_marginTop="@dimen/padding_19"
                    android:layout_width="@dimen/padding_154"
                    android:layout_height="@dimen/padding_115"
                    android:background="@drawable/bg_solid_88b0e3_4">

                    <ImageView
                        android:id="@+id/img_yyzz"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_pszp"
                        android:layout_centerInParent="true"/>

                </RelativeLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@id/rel_yyzz"
                    android:layout_alignEnd="@+id/rel_yyzz"
                    android:textSize="@dimen/text_size_13"
                    android:textColor="@color/color_666666"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="@dimen/padding_8"
                    android:layout_below="@+id/rel_yyzz"
                    android:text="营业执照"/>


                <RelativeLayout
                    android:id="@+id/rel_scgh"
                    android:layout_toEndOf="@+id/rel_yyzz"
                    android:layout_marginStart="@dimen/padding_19"
                    android:layout_marginTop="@dimen/padding_19"
                    android:layout_width="@dimen/padding_154"
                    android:layout_height="@dimen/padding_115"
                    android:background="@drawable/bg_solid_88b0e3_4">

                    <ImageView
                        android:id="@+id/img_scgh"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerInside"
                        android:src="@drawable/ic_scgh" />

                </RelativeLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@id/rel_scgh"
                    android:layout_alignEnd="@+id/rel_scgh"
                    android:textSize="@dimen/text_size_13"
                    android:textColor="@color/color_666666"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="@dimen/padding_8"
                    android:layout_below="@+id/rel_scgh"
                    android:text="商标注册证书"/>

            </RelativeLayout>

            <TextView
                android:id="@+id/tv_commit"
                android:layout_width="match_parent"
                android:layout_height="@dimen/padding_42"
                android:layout_marginStart="@dimen/padding_19"
                android:layout_marginEnd="@dimen/padding_19"
                android:layout_marginTop="@dimen/padding_30"
                android:text="提交申请"
                android:gravity="center"
                android:background="@drawable/bg_solid_f7d16c_21"
                android:textColor="#6c4304"
                android:layout_marginBottom="@dimen/padding_19"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>