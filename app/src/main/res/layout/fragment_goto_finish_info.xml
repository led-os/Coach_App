<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/padding_240"
    android:layout_height="wrap_content"

    tools:context=".ui.trainer.position.GotoFinishInfoFragment">

    <LinearLayout
        android:layout_width="@dimen/padding_240"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_solid_ffffff_6"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/tv_title"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/text_size_15"
            android:layout_marginTop="@dimen/padding_30"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="您还未完善个人信息" />

        <TextView
            android:id="@+id/tv_hint"
            android:text="完善个人信息"
            android:textSize="@dimen/text_size_14"
            android:textColor="@color/color_666666"
            android:layout_marginTop="@dimen/padding_30"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>


        <TextView
            android:id="@+id/tv_hint_1"
            android:text="可以发现更多的职位"
            android:textSize="@dimen/text_size_14"
            android:textColor="@color/color_666666"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tv_go_finish"
            android:layout_width="match_parent"
            android:layout_height="@dimen/padding_40"
            android:text="去完善"
            android:textSize="@dimen/text_size_14"
            android:gravity="center"
            android:layout_marginTop="@dimen/padding_60"
            android:textColor="@color/colorPrimary"/>



    </LinearLayout>

</LinearLayout>